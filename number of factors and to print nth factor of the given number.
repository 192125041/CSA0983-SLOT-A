import java.util.*;
class Main{
    public static void main(String[] args){
        int a=100, b=4, c=0, d=0;
        for(int i=1;i<=a;i++){
            if(a%i==0){
                c=c+1;
                if(c==b){
                    d=i;}}}
            System.out.println(c);
            System.out.println(d);}}
