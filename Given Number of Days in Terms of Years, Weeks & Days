import java.util.*;
class Main{
    public static void main(String[] args){
        int a=720;
        int b=a/365;
        int c=a%365;
        int d=c/7;
        int e=c%7;
        System.out.println(b);
        System.out.println(c);
        System.out.println(d);
        System.out.println(e);}}
