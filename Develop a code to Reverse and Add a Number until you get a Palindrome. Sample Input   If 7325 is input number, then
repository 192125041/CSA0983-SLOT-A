import java.util.*;
class Main{
public static void main(String [] args){
  int n=23;
int l=0;
while(l==0){
    int k=n;
    int s=0;
    while(n!=0){
       int  r=n%10;
        s=s*10+r;
        n=n/10;}
    int c=k+s;
    if(k==s){
        l=1;
        break;}
    else{
     System.out.println(k+" "+ s+" "+ c);
     n=c;}}}}
